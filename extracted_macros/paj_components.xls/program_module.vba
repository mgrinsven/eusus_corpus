Attribute VB_Name = "program_module"Option ExplicitSub PAJ_add()    PAJ_checkworksheet    MsgBox ("This subroutine would add a data form.")End SubSub PAJ_solve()    PAJ_checkworksheet    MsgBox ("This subroutine would solve the problem.")End SubSub PAJ_testfindworksheet()    Dim my_name As String, I_OK As Boolean    my_name = InputBox("Enter the name of the worksheet")    If my_name = "" Then Exit Sub    PAJ_findworksheet my_name, I_OK    End SubSub PAJ_test()    Dim pjrange As Range    PAJ_makematrix Cells(7, 8), 4, 10, True, 1, xlRight, , CMaroon, "00.00"    Set pjrange = Range("pppp")    PAJ_chgmatrix pjrange, 5, 6    'PAJ_makematrix(cell_start, s_length , s_width , s_horizontal , _     Optional cell_contents, Optional s_position, Optional cell_color, Optional border_color, Optional cell_format)End SubSub PAJ_testmakeseries()    PAJ_makeseries Range(Cells(2, 2), Cells(2, 3)), 10, True, "x", 1, xlCenter, LtYellow, CRed    PAJ_makeseries Range(Cells(2, 2), Cells(2, 3)), 10, False, "x", 1, xlRight    PAJ_makematrix Cells(3, 3), 2, 3, True, "=R[-1]C[-1]", xlCenter, LtYellow, 0    PAJ_makematrix Cells(7, 8), 2, 3, False, 1, xlRight, , CMaroon, "00.00"        'PAJ_makematrix(cell_start, s_length , s_width , s_horizontal , _     Optional cell_contents, Optional s_position, Optional cell_color, Optional border_color, Optional cell_format)End SubSub PAJ_testmakecolor()    Dim color_range As Range    Set color_range = Range(Cells(5, 5), Cells(10, 10))    PAJ_makecolor color_range, LtYellow    PAJ_makeborders color_range, CDkBlue    PAJ_allborders color_range    PAJ_side_borders color_range    End SubSub PAJ_relink()    PAJ_fixformulas    PAJ_testbuttonEnd SubSub PAJ_testbutton()    ActiveSheet.Buttons.Delete    PAJ_addbutton Cells(3, 3), "PAJ_add", "Add Form"    PAJ_addbutton Cells(6, 3), "PAJ_solve", "Solve"    Cells(1, 1).SelectEnd Sub