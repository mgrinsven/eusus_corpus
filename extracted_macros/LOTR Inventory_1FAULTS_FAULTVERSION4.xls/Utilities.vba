Attribute VB_Name = "Utilities"Public Sub makeurls()Attribute makeurls.VB_ProcData.VB_Invoke_Func = "y\r14"    lastrow = ActiveSheet.UsedRange.Rows.Count - 1    thisset = "00"        While ActiveSheet.Hyperlinks.Count > 0        ActiveSheet.Hyperlinks(1).Delete    Wend        For i = 2 To lastrow        If i < 11 Then            Row = "00" & i - 1        ElseIf i < 101 Then            Row = "0" & i - 1        Else            Row = i - 1        End If                ActiveSheet.Hyperlinks.Add ActiveSheet.Cells(i, 3), _            "http://shop.decipher.com/Images/CardImages/LOTR-EN" & thisset & Row & ".jpg"                    Next i        End SubPublic Sub removespaces()    For Each sheet In ActiveWorkbook.Sheets        For Each cell In sheet.UsedRange            cell.Formula = mytrim(cell.Formula)        Next cell    Next sheet    End SubPublic Function mytrim(str As String) As String    Dim done As Boolean    done = False    While Not done        Select Case Left$(str, 1)            Case " ", vbTab, vbCr, vbLf, Chr$(202)                str = Mid$(str, 2)            Case Else                done = True        End Select    Wend        done = False    While Not done        Select Case Right$(str, 1)            Case " ", vbTab, vbCr, vbLf, Chr$(202)                str = Mid$(str, 1, Len(str) - 1)            Case Else                done = True        End Select    Wend    mytrim = strEnd FunctionPublic Sub test()    MsgBox Asc(Right$(Range("c4").Formula, 1))End Sub